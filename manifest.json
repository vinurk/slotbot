{
    "name": "SlotBot",
    "description": "It reserves a slot when it appears.",
    "version": "1.0",
    "manifest_version": 3,
    "icons":{
        "16":"/icons/click_16.png",
        "48":"/icons/click_48.png",
        "128":"/icons/click_128.png"
    },
    "permissions":["storage"],
    "action":{
        "default_icon":{
            "16":"/icons/click_16.png",
            "48":"/icons/click_48.png",
            "128":"/icons/click_128.png"
        },
        "default_popup":"popup.html"
        
    },
    "content_scripts":[
        {
        "matches":["<all_urls>"],
        "js":["content.js"]
        }
]
  }